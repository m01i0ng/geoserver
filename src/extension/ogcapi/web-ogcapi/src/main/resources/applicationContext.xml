<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="ogcapiLayerConfig" class="org.geoserver.web.publish.LayerConfigurationPanelInfo">
        <property name="id" value="ogcapiLayerConfig"/>
        <property name="titleKey" value="data.resource.config.ogcapi"/>
        <property name="componentClass" value="org.geoserver.web.ogcapi.OGCAPILayerConfig"/>
    </bean>

    <bean id="ogcapiLinksSettings" class="org.geoserver.web.data.settings.SettingsPluginPanelInfo">
        <property name="id" value="ogcapiLinksSettings"/>
        <property name="titleKey" value="ogcapiLinksSettings"/>
        <property name="componentClass" value="org.geoserver.web.ogcapi.OGCAPILinksSettingsPanel"/>
    </bean>

</beans>
