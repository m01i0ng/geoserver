<assembly>
  <id>acl-plugin</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>target</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>${artifactId}-${project.version}.jar</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>target/dependency</directory>
      <outputDirectory></outputDirectory>
      <!-- include dependencies listed by `mvn dependency:list|grep ":compile"|sort -u` in community/acl -->
      <includes>
        <include>gs-acl-*.jar</include>
        <include>geolatte-geom-*.jar</include>
        <include>jackson-dataformat-smile-*.jar</include>
        <include>jackson-datatype-jsr310-*.jar</include>
        <include>caffeine-*.jar</include>
        <include>jsr305-*.jar</include>
        <include>error_prone_annotations-*.jar</include>
        <include>swagger-annotations-*.jar</include>
        <include>validation-api-*.jar</include>
        <include>checker-qual-*.jar</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
